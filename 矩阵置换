#include <stdio.h>
#include <string.h>
int main()
{
    int sz[6][6],n,i,j,k;
    printf("Enter n:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
            scanf("%d",&sz[i][j]);
    }
     for(j=0;j<n;j++)
    {
        for(i=0;i<n;i++)
        {
            printf("%4d",sz[i][j]);
        }
        printf("\n");
    }
    return 0;
}
