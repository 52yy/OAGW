#include <stdio.h>
int main()
{
    int sz[3][2],i,n,j,max,k,l;
    printf("Enter 6 integers:\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        {
            scanf("%d",&sz[i][j]);
        }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        {
           printf("%4d",sz[i][j]);
        }
        printf("\n");
     }
    max=sz[0][0]ï¼›
    k=0;
    l=0;
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        {
            if(max<sz[i][j])
            {
                max=sz[i][j];
                k=i;
                l=j;
            }
        }
    }
    printf("max=a[%d][%d]=%d",k,l,max);
    return 0;
}
