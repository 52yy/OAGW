#include <stdio.h>
int main()
{
    int a,i,j,k;
    scanf("%d",&a);
    printf("\n");
    for(i=0;i<a;i++)
    {
        for(j=0;j<i;j++)
        {
            printf(" ");
        }
        for(k=i;k<2*a-i-1;k++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
