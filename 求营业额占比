#include<stdio.h>
int main()
{
    int i,j,k;
    double a[12],s=0;
    for(i=0;i<12;i++)
    {
        scanf("%lf",&a[i]);
        s=s+a[i];
    }
    for(i=0;i<12;i++)
    {
        printf("%2d",i+1);
        int(k)=100*(a[i]/s+0.005);
        printf("(%2d",k);
        printf("%%) ");
        for(j=0;j<k;j++)
            printf("#");
        printf("\n");
    }
    return 0;
}
