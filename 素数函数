#include <stdio.h>
int prime(int n)
{
    int j,i,k=0ï¼›
    for(i=2;i<n;i++)
    {
        if(n==2)
            k=0;
        else if(n%i==0)
            k++;
    }
    if(!k)
        return 0;
    else
        return 1;
}
int main()
{
    int n,j=0;
    for(n=2;n<100;n++)
        if(!prime(n))
        {
            j++;
            printf("%6d",n);
            if(j%10==0)
                printf("\n");
        }
    return 0;
}
